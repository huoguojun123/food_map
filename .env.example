# AI Configuration (User configurable in UI, default values)
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o
# Optional: split models for better speed/cost
OPENAI_TEXT_MODEL=
OPENAI_VISION_MODEL=
# Optional: default 120000ms in code (can override here)
OPENAI_TIMEOUT_MS=120000
# Optional: settings page connectivity test timeout
OPENAI_TEST_TIMEOUT_MS=20000

# Map API (AMap - 高德地图)
AMAP_KEY=

# Frontend
# Optional: when empty, production uses same origin; local dev auto-falls back to http://127.0.0.1:3001
NEXT_PUBLIC_API_URL=

# Cloudflare Services (Optional - for R2 and D1)
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_D1_DATABASE_ID=
CLOUDFLARE_API_TOKEN=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_ACCOUNT_ID=
R2_BUCKET_NAME=
R2_PUBLIC_URL=

# API Server (Bun)
API_PORT=3001
API_HOST=127.0.0.1
